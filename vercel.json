            "dest": "/api/index.php"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
